    "        if any(palabra in c.lower() for palabra in palabras_clave_id)\n",
    "    ]\n",
    "    resumen_general['posibles_identificadores'] = posibles_identificadores\n",
    "    \n",
    "    # =====================================================\n",
    "    # PARTE 2: ANALISIS DETALLADO POR COLUMNA\n",
